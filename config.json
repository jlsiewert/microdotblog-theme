{
    "markup":  {
         "defaultMarkdownHandler": "goldmark",
        "goldmark": {
             "extensions": {
                "definitionList": true,
                "footnote": true,
                "linkify": false,
                "linkifyProtocol": "https",
                "strikethrough": true,
                "table": true,
                "taskList": true,
                "typographer": true
             },
             "parser": {
                "attribute": {
                   "block": false,
                   "title": true
                },
                "autoHeadingID": false,
                "autoHeadingIDType": "github"
             },
             "renderer": {
                "hardWraps": false,
                "unsafe": false,
                "xhtml": false
             }
          }
      }
    }
    